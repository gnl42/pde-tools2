<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="me.glindholm.pdetools2.icg.builder"
         name="Shared Image Generator"
         point="org.eclipse.core.resources.builders">
      <builder
            callOnEmptyDelta="false"
            hasNature="true"
            isConfigurable="true"
            supportsConfigurations="true">
         <run
               class="me.glindholm.pdetools2.icg.builder.ICGBuilder">
         </run></builder>
   </extension>
   <extension
         id="me.glindholm.pdetools2.icg.nature"
         name="Managed Shared Image Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="me.glindholm.pdetools2.icg.builder.ICGNature">
         </run>
      </runtime>
      <requires-nature
            id="org.eclipse.pde.PluginNature">
      </requires-nature>
      <builder
            id="me.glindholm.pdetools2.icg.builder">
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="me.glindholm.pdetools2.category"
            name="PDE Tools">
      </category>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.icg.enableNature"
            name="Enable Shared Image Generator">
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.icg.disableNature"
            name="Disable Shared Image Generator">
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.clipboard.pasteHistory"
            name="Paste Content in History">
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            defaultHandler="me.glindholm.pdetools2.snapshot.TakeScreenShotHandler"
            id="me.glindholm.pdetools2.snapshot.screenshot"
            name="Take a Screen Shot">
      </command>
      <command
            defaultHandler="me.glindholm.pdetools2.wslauncher.LaunchWorkspaceHandler"
            id="me.glindholm.pdetools2.wslauncher.launchworkspace"
            name="Launch Workspace">
         <commandParameter
               id="workspace"
               name="workspace"
               optional="true">
         </commandParameter>
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.command.showInShell"
            name="Show in Shell">
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.command.openConsole"
            name="Open Console Here">
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.command.copy.resource.path"
            name="Copy Resource Full Path">
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.shell.CopyRelativeResourcePath"
            name="Copy Relative Resource Path">
      </command>
      <command
            categoryId="me.glindholm.pdetools2.category"
            id="me.glindholm.pdetools2.shell.CopyReversedRelativeResourcePath"
            name="Copy Relative Resource Path">
      </command>
      <command
            defaultHandler="me.glindholm.pdetools2.commandspy.ToggleGlobalCommandSpyHandler"
            id="me.glindholm.pdetools2.command.toggleGlobalCommandSpy"
            name="Toggle Global Command Spy">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="me.glindholm.pdetools2.clipboard.pasteHistory"
            icon="icons/clipboard.gif">
      </image>
      <image
            commandId="me.glindholm.pdetools2.icg.enableNature"
            icon="icons/image-folder.gif">
      </image>
      <image
            commandId="me.glindholm.pdetools2.snapshot.screenshot"
            icon="icons/camera.gif">
      </image>
      <image
            commandId="me.glindholm.pdetools2.command.toggleGlobalCommandSpy"
            icon="icons/spy.png">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="me.glindholm.pdetools2.icg.enableNature">
         <class
               class="me.glindholm.pdetools2.icg.builder.EnableICGNatureHandler"></class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.icg.disableNature">
         <class
               class="me.glindholm.pdetools2.icg.builder.DisableICGNatureHandler"></class>
      </handler>
      <handler
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <and>
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="me.glindholm.pdetools2.model.pdetools.ClipboardEntry">
                  </instanceof>
               </iterate>
            </and>
         </activeWhen>
         <enabledWhen>
            <and>
               <iterate>
                  <instanceof
                        value="me.glindholm.pdetools2.model.pdetools.ClipboardEntry">
                  </instanceof>
               </iterate>
               <count
                     value="+">
               </count>
            </and>
         </enabledWhen>
         <class
               class="me.glindholm.pdetools2.clipboard.internal.RemoveClipboardEntryHandler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.clipboard.pasteHistory">
         <activeWhen>
            <with
                  variable="me.glindholm.pdetools2.workbench">
               <test
                     args="me.glindholm.pdetools2.capability.clipboard"
                     property="me.glindholm.pdetools2.isActivityEnabled">
               </test>
            </with>
         </activeWhen>
         <class
               class="me.glindholm.pdetools2.clipboard.internal.PasteFromClipboardHistoryHandler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.command.showInShell">
         <activeWhen>
            <with
                  variable="me.glindholm.pdetools2.platform">
               <test
                     property="me.glindholm.pdetools2.os"
                     value="macosx">
               </test>
            </with>
         </activeWhen>
         <class
               class="me.glindholm.pdetools2.shell.ShowInShellOSXHandler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.command.showInShell">
         <activeWhen>
            <with
                  variable="me.glindholm.pdetools2.platform">
               <test
                     property="me.glindholm.pdetools2.os"
                     value="win32">
               </test>
            </with>
         </activeWhen>
         <class
               class="me.glindholm.pdetools2.shell.ShowInShellWin32Handler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.command.showInShell">
         <activeWhen>
            <with
                  variable="me.glindholm.pdetools2.platform">
               <test
                     property="me.glindholm.pdetools2.os"
                     value="linux">
               </test>
            </with>
         </activeWhen>
         <class
               class="me.glindholm.pdetools2.shell.ShowInShellLinuxHandler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.command.openConsole">
         <activeWhen>
            <with
                  variable="me.glindholm.pdetools2.platform">
               <test
                     property="me.glindholm.pdetools2.os"
                     value="macosx">
               </test>
            </with>
         </activeWhen>
         <class
               class="me.glindholm.pdetools2.shell.OpenConsoleOSXHandler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.command.openConsole">
         <activeWhen>
            <with
                  variable="me.glindholm.pdetools2.platform">
               <test
                     property="me.glindholm.pdetools2.os"
                     value="win32">
               </test>
            </with>
         </activeWhen>
         <class
               class="me.glindholm.pdetools2.shell.OpenConsoleWin32Handler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.command.openConsole">
         <activeWhen>
            <with
                  variable="me.glindholm.pdetools2.platform">
               <test
                     property="me.glindholm.pdetools2.os"
                     value="linux">
               </test>
            </with>
         </activeWhen>
         <class
               class="me.glindholm.pdetools2.shell.OpenConsoleLinuxHandler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.command.copy.resource.path">
         <class
               class="me.glindholm.pdetools2.shell.CopyResourcePathHandler">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.shell.CopyRelativeResourcePath">
         <class
               class="me.glindholm.pdetools2.shell.CopyRelativeResourcePath">
         </class>
      </handler>
      <handler
            commandId="me.glindholm.pdetools2.shell.CopyReversedRelativeResourcePath">
         <class
               class="me.glindholm.pdetools2.shell.CopyReversedRelativeResourcePath">
         </class>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="me.glindholm.pdetools2.snapshot.toolbar"
               label="Snapshot Toolbar">
            <command
                  commandId="me.glindholm.pdetools2.snapshot.screenshot"
                  id="me.glindholm.pdetools2.snapshot.toolbar.screenshot"
                  style="pulldown">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="me.glindholm.pdetools2.workbench">
                     <test
                           args="me.glindholm.pdetools2.capability.snapshot"
                           property="me.glindholm.pdetools2.isActivityEnabled">
                     </test>
                  </with>
               </visibleWhen>
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="me.glindholm.pdetools2.workbench">
                  <test
                        args="me.glindholm.pdetools2.capability.snapshot"
                        property="me.glindholm.pdetools2.isActivityEnabled">
                  </test>
               </with>
            </visibleWhen>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:me.glindholm.pdetools2.snapshot.toolbar.screenshot">
         <command
               commandId="me.glindholm.pdetools2.snapshot.screenshot"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.views.showView"
               icon="icons/pictures.gif"
               label="Show Camera Roll"
               style="push">
            <parameter
                  name="org.eclipse.ui.views.showView.viewId"
                  value="me.glindholm.pdetools2.snapshot.SnapshotView">
            </parameter>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.projectConfigure">
         <command
               commandId="me.glindholm.pdetools2.icg.enableNature"
               icon="icons/image-folder.gif"
               label="Enable Shared Image Generator"
               style="push">
            <visibleWhen
                  checkEnabled="false">
                  <iterate
                           ifEmpty="false"
                           operator="and">
                        <adapt
                              type="org.eclipse.core.resources.IProject">
                           <instanceof
                                 value="org.eclipse.core.resources.IProject">
                           </instanceof>
                           <test
                                 property="org.eclipse.core.resources.projectNature"
                                 value="org.eclipse.pde.PluginNature">
                           </test>
                           <not>
                              <test
                                    property="org.eclipse.core.resources.projectNature"
                                    value="me.glindholm.pdetools2.icg.nature">
                              </test>
                           </not>
                        </adapt>
                     </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="me.glindholm.pdetools2.icg.disableNature"
               icon="icons/image-folder.gif"
               label="Disable Shared Image Generator"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                     <instanceof
                           value="org.eclipse.core.resources.IProject">
                     </instanceof>
                     <test
                           property="org.eclipse.core.resources.projectNature"
                           value="org.eclipse.pde.PluginNature">
                     </test>
                     <test
                           property="org.eclipse.core.resources.projectNature"
                           value="me.glindholm.pdetools2.icg.nature">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=paste">
         <command
               commandId="me.glindholm.pdetools2.clipboard.pasteHistory"
               icon="icons/clipboard.gif"
               id="me.glindholm.pdetools2.clipboard.menu.pasteHistory"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="me.glindholm.pdetools2.workbench">
                  <test
                        args="me.glindholm.pdetools2.capability.clipboard"
                        property="me.glindholm.pdetools2.isActivityEnabled">
                  </test>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:me.glindholm.pdetools2.clipboard.ClipboardView">
         <command
               commandId="org.eclipse.ui.edit.delete"
               style="push">
         </command>
         <separator
               name="additions"
               visible="true">
         </separator>
         <command
               commandId="org.eclipse.ui.file.properties"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:me.glindholm.pdetools2.clipboard.ClipboardView">
         <command
               commandId="org.eclipse.ui.window.preferences"
               style="push">
            <parameter
                  name="preferencePageId"
                  value="me.glindholm.pdetools2.clipboard.internal.ClipboardPreferencePage">
            </parameter>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?before=org.eclipse.ui.file.restartWorkbench">
         <menu
               id="me.glindholm.pdetools2.wslauncher.launchworkspace"
               label="Launch New Workspace">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="me.glindholm.pdetools2.workbench">
                  <test
                        args="me.glindholm.pdetools2.capability.wslauncher"
                        property="me.glindholm.pdetools2.isActivityEnabled">
                  </test>
               </with>
            </visibleWhen>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:me.glindholm.pdetools2.wslauncher.launchworkspace">
         <dynamic
               class="me.glindholm.pdetools2.wslauncher.LaunchWorkspaceMenu"
               id="me.glindholm.pdetools2.wslauncher.LaunchWorkspaceMenu">
         </dynamic>
         <command
               commandId="me.glindholm.pdetools2.wslauncher.launchworkspace"
               id="me.glindholm.pdetools2.wslauncher.launchworkspace.other"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.window.preferences"
               label="Launch Options"
               style="push">
            <parameter
                  name="preferencePageId"
                  value="me.glindholm.pdetools2.wslauncher.WSLauncherPreferencePage">
            </parameter>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="me.glindholm.pdetools2.shell.menu"
               label="PDE-Tools Shell">
            <visibleWhen
                  checkEnabled="true">
               <and>
                  <with
                        variable="me.glindholm.pdetools2.workbench">
                     <test
                           args="me.glindholm.pdetools2.capability.shell"
                           property="me.glindholm.pdetools2.isActivityEnabled">
                     </test>
                  </with>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                        <instanceof
                              value="org.eclipse.core.resources.IResource">
                        </instanceof>
                     </adapt>
                  </iterate>
                  <or>
                     <count
                           value="1">
                     </count>
                     <count
                           value="2">
                     </count>
                  </or>
               </and>
            </visibleWhen>
            <command
                  commandId="me.glindholm.pdetools2.command.copy.resource.path"
                  icon="icons/copy.gif"
                  id="me.glindholm.pdetools2.shell.menu.copy.resource.path"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
                  <and>
                     <with
                           variable="me.glindholm.pdetools2.workbench">
                        <test
                              args="me.glindholm.pdetools2.capability.shell"
                              property="me.glindholm.pdetools2.isActivityEnabled">
                        </test>
                     </with>
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false"
                           operator="and">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <instanceof
                                 value="org.eclipse.core.resources.IResource">
                           </instanceof>
                        </adapt>
                     </iterate>
                  </and>
               </visibleWhen>
            </command>
            <command
                  commandId="me.glindholm.pdetools2.shell.CopyRelativeResourcePath"
                  icon="icons/copy.gif"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
                  <and>
                     <with
                           variable="me.glindholm.pdetools2.workbench">
                        <test
                              args="me.glindholm.pdetools2.capability.shell"
                              property="me.glindholm.pdetools2.isActivityEnabled">
                        </test>
                     </with>
                     <count
                           value="2">
                     </count>
                     <iterate
                           ifEmpty="false"
                           operator="and">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <instanceof
                                 value="org.eclipse.core.resources.IResource">
                           </instanceof>
                        </adapt>
                     </iterate>
                  </and>
               </visibleWhen>
            </command>
            <command
                  commandId="me.glindholm.pdetools2.shell.CopyReversedRelativeResourcePath"
                  icon="icons/copy.gif"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
                  <and>
                     <with
                           variable="me.glindholm.pdetools2.workbench">
                        <test
                              args="me.glindholm.pdetools2.capability.shell"
                              property="me.glindholm.pdetools2.isActivityEnabled">
                        </test>
                     </with>
                     <count
                           value="2">
                     </count>
                     <iterate
                           ifEmpty="false"
                           operator="and">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <instanceof
                                 value="org.eclipse.core.resources.IResource">
                           </instanceof>
                        </adapt>
                     </iterate>
                  </and>
               </visibleWhen>
            </command>
            <command
                  commandId="me.glindholm.pdetools2.command.showInShell"
                  id="me.glindholm.pdetools2.shell.menu.showInShell"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
                  <and>
                     <with
                           variable="me.glindholm.pdetools2.workbench">
                        <test
                              args="me.glindholm.pdetools2.capability.shell"
                              property="me.glindholm.pdetools2.isActivityEnabled">
                        </test>
                     </with>
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false"
                           operator="and">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <instanceof
                                 value="org.eclipse.core.resources.IResource">
                           </instanceof>
                        </adapt>
                     </iterate>
                  </and>
               </visibleWhen>
            </command>
            <command
                  commandId="me.glindholm.pdetools2.command.openConsole"
                  icon="icons/terminal.png"
                  id="me.glindholm.pdetools2.shell.menu.openConsole"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
                  <and>
                     <with
                           variable="me.glindholm.pdetools2.workbench">
                        <test
                              args="me.glindholm.pdetools2.capability.shell"
                              property="me.glindholm.pdetools2.isActivityEnabled">
                        </test>
                     </with>
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false"
                           operator="and">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <instanceof
                                 value="org.eclipse.core.resources.IResource">
                           </instanceof>
                        </adapt>
                     </iterate>
                  </and>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:me.glindholm.pdetools2.commandspy.GlobalCommandSpyView">
         <command
               commandId="me.glindholm.pdetools2.command.toggleGlobalCommandSpy"
               style="toggle">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="me.glindholm.pdetools2.icg.ui.ICGPropertyPage"
            id="me.glindholm.pdetools2.icg.propertyPage"
            name="Shared Image Generator">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <test
                     property="org.eclipse.core.resources.projectNature"
                     value="me.glindholm.pdetools2.icg.nature">
               </test>
            </adapt>
         </enabledWhen>
      </page>
      <page
            class="me.glindholm.pdetools2.clipboard.ClipEntryPropertyPage"
            id="me.glindholm.pdetools2.clipboard.ClipEntryPropertyPage"
            name="Clipboard Entry">
         <enabledWhen>
            <instanceof
                  value="me.glindholm.pdetools2.model.pdetools.ClipboardEntry">
            </instanceof>
         </enabledWhen>
      </page>
   </extension>
   <extension
         id="me.glindholm.pdetools2.icg.problemmarker"
         name="Shared Image Generator Problem Marker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="me.glindholm.pdetools2.icg.builder.ResolutionProvider"
            markerType="me.glindholm.pdetools2.icg.problemmarker">
      </markerResolutionGenerator>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            class="me.glindholm.pdetools2.decorator.IconDecorator"
            icon="icons/image.gif"
            id="me.glindholm.pdetools2.icondecorator"
            label="PDE Tools - Icon File Decorator"
            lightweight="true"
            location="REPLACE"
            state="true">
         <enablement>
            <objectClass
                  name="org.eclipse.core.resources.IFile">
            </objectClass>
         </enablement>
      </decorator>
      <decorator
            class="me.glindholm.pdetools2.decorator.MonitorFolderDecorator"
            icon="icons/image-folder.gif"
            id="me.glindholm.pdetools2.icg.monitorfolder"
            label="PDE Tools - Shared Image Folder"
            lightweight="true"
            location="REPLACE"
            state="true">
         <enablement>
            <and>
               <objectClass
                     name="org.eclipse.core.resources.IFolder">
               </objectClass>
               <objectState
                     name="projectNature"
                     value="me.glindholm.pdetools2.icg.nature">
               </objectState>
            </and>
         </enablement>
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ltk.core.refactoring.renameParticipants">
      <renameParticipant
            class="me.glindholm.pdetools2.icg.refactor.RenameMonitorFolder"
            id="me.glindholm.pdetools2.icg.renameParticipant"
            name="Rename Monitoring Folder">
         <enablement>
            <with
                  variable="element">
               <adapt
                     type="org.eclipse.core.resources.IResource">
